{
  "name": "automatisch",
  "version": "1.0.0",
  "description": "Web driver io, cucumber automation layout",
  "main": "auto.js",
  "scripts": {
    "compile": "babel wdio.es6 --out-dir ./wdio.compiled",
    "wdio": "wdio ./wdio.compiled/bootstrap-wdio-config.js",
    "start": "npm run compile && npm run wdio"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "chai"
  ],
  "author": "senthilkumar baliah",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "wdio-allure-reporter": "^0.3.2",
    "wdio-cucumber-framework": "^1.1.0",
    "wdio-json-reporter": "^0.3.1",
    "wdio-junit-reporter": "^0.3.1",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.10.2"
  },
  "dependencies": {
    "chai": "^4.1.2"
  }
}
